   "source": [
    "import sympy as sp\n",
    "\n",
    "Ya , Yb, Yc, I1, V1, I2, V2 = sp.symbols('Ya Yb Yc I1 V1 I2 V2')"
   ]
  },
  {
   "cell_type": "code",
